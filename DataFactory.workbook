    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "let T = ADFPipelineRun\r\n| where ResourceId in ({DataFactory}) \r\n| summarize arg_max(TimeGenerated,* ) by RunId\r\n| extend StatusIcon = case(Status == \"Succeeded\", 'üü¢', \r\n                       Status == \"InProgress\", 'üü°', \r\n                       Status == \"Queued\", 'üîµ',\r\n                       Status == \"Cancelled\",'üü†', \r\n                       Status == \"Failed\", 'üî¥', \r\n                       '0')\r\n|order by TimeGenerated;\r\n\r\n\r\n\r\n\r\nlet T2 = T\r\n| summarize commands_details = make_list(pack('status', StatusIcon, 'TimeGenerated', TimeGenerated)) by PipelineName\r\n| mv-apply command_details = commands_details on\r\n(\r\n    order by todatetime(command_details['TimeGenerated']) asc\r\n    | summarize make_list(command_details['status'])\r\n)\r\n| project-away commands_details \r\n| extend ['Last runs'] = strcat_array(list_command_details_status,'');\r\n\r\n\r\n\r\n\r\nADFPipelineRun\r\n| where ResourceId in ({DataFactory}) \r\n| summarize arg_max(TimeGenerated,* ) by PipelineName\r\n| extend ParentId = '', Id = PipelineName,Link =  strcat(\"https://ms-adf.azure.com/monitoring/pipelineruns?factory=\", ResourceId), Resource = ResourceId, endd = iff(End == todatetime('1601-01-01T00:00:00Z'),now(),End)\r\n| extend Duration = format_timespan(endd- Start,'hh:mm:ss') ,Pipeline =  strcat('üóúÔ∏è ', PipelineName) \r\n| join  T2 on PipelineName\r\n|project ParentId, Id , Resource, Pipeline, Status, Start, Duration, ['Last runs'], Link,TimeGenerated\r\n\r\n|union (T\r\n| extend ParentId = PipelineName,Id = RunId,Link = strcat(\"https://ms-adf.azure.com/monitoring/pipelineruns/\",CorrelationId\r\n,\"?factory=\",ResourceId), endd = iff(End == todatetime('1601-01-01T00:00:00Z'),now(),End)\r\n|extend Duration = format_timespan(endd- Start,'hh:mm:ss')  \r\n|project ParentId,Id, Status,Start,Duration,Link,TimeGenerated)\r\n|union (ADFActivityRun \r\n| where ResourceId in ({DataFactory}) \r\n| summarize arg_max(TimeGenerated,* ) by ActivityRunId\r\n| extend endd = iff(End == todatetime('1601-01-01T00:00:00Z'),now(),End)\r\n| extend  ParentId = PipelineRunId,Id = ActivityRunId,Link=\"\",Duration = format_timespan(endd- Start,'hh:mm:ss')  , Resource = ActivityName\r\n| order by TimeGenerated desc \r\n| project ParentId,Id, Status,Start,Duration,Link,TimeGenerated, Resource)\r\n| union (ADFActivityRun \r\n| where ResourceId in ({DataFactory}) \r\n| summarize arg_max(TimeGenerated,* ) by ActivityRunId\r\n| where Status ==\"Failed\"\r\n| extend  ParentId = ActivityRunId, Id = \"*\", ['Last runs'] = ErrorMessage\r\n| project ParentId, Id, ['Last runs']);\r\n",
